@model IEnumerable<pointOfSales.Models.Role>

@{
    ViewBag.Title = "Index";
}

<h2>

    Existing Roles in The System
</h2>
<p>

    <i class="fa fa-plus" aria-hidden="true">@Html.ActionLink(" Add New Role", "Create")</i>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserType)
        </td>
        <td>
        <td>
            <i class="fa fa-pencil" aria-hidden="true"> @Html.ActionLink("Edit", "Edit", new { id = item.RoleID }) |</i>
            <i class="fa fa-eye" aria-hidden="true"> @Html.ActionLink("Details", "Details", new { id = item.RoleID }) |</i>
            <i class="fa fa-trash" aria-hidden="true"> @Html.ActionLink("Delete", "Delete", new { id = item.RoleID })</i>
